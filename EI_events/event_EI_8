        <div class="infos">
            <div class="date">
                <span class="date-day">Tue</span>
                <span class="date-d">22</span>
                <span class="date-month">Nov</span>
                            </div>
            <span class="time">13:00</span>
            <div class="location">
                <span class="city">York</span>
                <span class="place">University of York</span>
            </div>
            <span class="title">YORK ACADEMY</span>
            <span class="description">Transmission et création </span>
                                </div>
    </a>
